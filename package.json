{
  "name": "react-helmet-async",
  "version": "2.0.5",
  "description": "Thread-safe Helmet for React 16+ and friends",
  "sideEffects": false,
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "typings": "./lib/index.d.ts",
  "repository": "http://github.com/staylor/react-helmet-async",
  "author": "Scott Taylor <scott.c.taylor@mac.com>",
  "license": "Apache-2.0",
  "files": [
    "lib/"
  ],
  "dependencies": {
    "invariant": "^2.2.4",
    "react-fast-compare": "^3.2.2",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@remix-run/eslint-config": "2.16.8",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/eslint": "9.6.1",
    "@types/invariant": "2.2.37",
    "@types/jsdom": "21.1.7",
    "@types/react": "19.1.8",
    "@types/shallowequal": "1.1.5",
    "@vitejs/plugin-react": "4.6.0",
    "esbuild": "0.25.6",
    "eslint": "9.30.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.5.1",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "prettier": "3.6.2",
    "raf": "3.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rimraf": "6.0.1",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vite": "7.0.4",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "react": "^19.1.0"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --report-unused-disable-directives .",
    "lint-fix": "yarn lint --fix",
    "test": "vitest run",
    "test-watch": "yarn test --watch",
    "test-update": "yarn test -u",
    "compile": "yarn run clean && NODE_ENV=production tsx build.ts && yarn types",
    "prepare": "yarn compile && husky install",
    "types": "tsc src/index.tsx --jsx react --declaration --esModuleInterop --allowJs --emitDeclarationOnly --outDir lib"
  }
}
